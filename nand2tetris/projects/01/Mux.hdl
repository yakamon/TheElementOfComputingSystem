/**
 * Multiplexor:
 * out = a if sel == 0
 *       b otherwise
 */

CHIP Mux {
    IN a, b, sel;
    OUT out;

    PARTS:
    Not(in=sel, out=nS);
    And(a=a, b=nS, out=andAnS);
    And(a=b, b=sel, out=andBS);
    Or(a=andAnS, b=andBS, out=out);
}