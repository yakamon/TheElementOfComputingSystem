/**
 * Exclusive-or gate:
 * out = not (a == b)
 */

CHIP Xor {
    IN a, b;
    OUT out;

    PARTS:
    Nand(a=a, b=a, out=nandAA);
    Nand(a=nandAA, b=b, out=nandAAB);
    Nand(a=b, b=b, out=nandBB);
    Nand(a=nandBB, b=a, out=nandBBA);
    Nand(a=nandAAB, b=nandBBA, out=out);
}